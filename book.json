{
  "plugins": [
    "page-toc",
    "-search",
    "-search-plus",
    "-sharing",
    "-lunr"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "fontsettings": {
      "theme": "night"
    },
    "page-toc": {
      "showByDefault": true,
      "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
      "position": "before-first"
    }
  }
}
