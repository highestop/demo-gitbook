{
  "title": "demo-gitbook",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "Github": "https://github.com/highestop/demo-gitbook"
    }
  },
  "styles": {
    "website": "styles/website.css"
  },
  "plugins": [
    "code",
    "back-to-top-button",
    "flexible-alerts",
    "hide-element",
    "insert-logo",
    "page-toc",
    "popup",
    "search-pro",
    "-search",
    "-search-plus",
    "-sharing",
    "-lunr"
  ],
  "pluginsConfig": {
    "fontsettings": {
      "theme": "night"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "insert-logo": {
      "url": "https://github.com/highestop/GitbookTemplate/blob/master/src/assets/favlogo.ico"
    },
    "page-toc": {
      "showByDefault": true,
      "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
      "position": "before-first"
    },
    "theme-default": {
      "showLevel": false
    }
  }
}
